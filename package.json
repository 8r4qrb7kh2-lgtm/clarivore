{
  "name": "cle-allergy-aware",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:next": "next dev",
    "build": "prisma generate && next build",
    "build:next": "prisma generate && next build",
    "preview": "next start -p 8081",
    "preview:next": "next start -p 8081",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "deploy": "bash scripts/deploy-vercel.sh",
    "deploy:with-kb": "node scripts/build-and-push-help-kb.mjs && bash scripts/deploy-vercel.sh",
    "cap:copy": "npm run build && npx cap copy",
    "cap:sync": "npm run build && npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "verify:next-transition": "bash -lc 'set -a; [ -f .env ] && source .env; [ -f .env.local ] && source .env.local; set +a; node scripts/verify-next-transition.mjs'",
    "verify:legacy-editor-parity": "node scripts/verify-legacy-editor-parity.mjs",
    "capture:menu-overlay-fixtures": "node scripts/capture-menu-overlay-fixtures.mjs",
    "verify:menu-overlay-parity": "node scripts/verify-menu-overlay-engine-parity.mjs",
    "ml:export": "python3 scripts/ml/export_training_data.py",
    "ml:fetch:off": "python3 scripts/ml/fetch_openfoodfacts_data.py",
    "ml:train": "python3 scripts/ml/train_fast_model.py",
    "ml:eval": "python3 scripts/ml/evaluate_model.py",
    "ml:pipeline": "npm run ml:export && npm run ml:train && npm run ml:eval",
    "ml:pipeline:online": "npm run ml:fetch:off && python3 scripts/ml/export_training_data.py --manual-labels scripts/ml/manual_hidden_alias_seed.jsonl --manual-labels ml/data/processed/openfoodfacts_examples.jsonl && npm run ml:train && npm run ml:eval",
    "docs:lint": "markdownlint-cli2 \"docs/manager-user-guide.md\" \"docs/manager-flows/**/*.md\"",
    "docs:flows:render": "node scripts/docs/render-manager-flow-diagrams.mjs",
    "docs:flows:optimize": "node scripts/docs/optimize-manager-flow-diagrams.mjs",
    "docs:flows:build": "npm run docs:flows:render && npm run docs:flows:optimize",
    "docs:flows:capture": "node scripts/docs/capture-manager-flow-screenshots.mjs",
    "docs:manager:pdf": "md-to-pdf docs/manager-user-guide.md",
    "axon:bootstrap": "bash scripts/axon-mcp/bootstrap.sh",
    "axon:up": "bash scripts/axon-mcp/up.sh",
    "axon:down": "bash scripts/axon-mcp/down.sh",
    "axon:index": "bash scripts/axon-mcp/index-worktree.sh",
    "axon:status": "bash scripts/axon-mcp/status.sh",
    "axon:ports": "bash scripts/axon-mcp/configure-ports.sh",
    "axon:codex": "bash scripts/axon-mcp/configure-codex-mcp.sh",
    "axon:cursor": "bash scripts/axon-mcp/configure-cursor-mcp.sh",
    "axon:setup": "bash scripts/axon-mcp/setup-all.sh"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@capacitor/app": "^6.0.3",
    "@capacitor/browser": "^6.0.6",
    "@capacitor/core": "^6.1.0",
    "@capacitor/ios": "^6.2.1",
    "@capacitor/push-notifications": "^6.0.4",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "autoprefixer": "^10.4.24",
    "next": "^14.2.35",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.0",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@playwright/test": "^1.58.2",
    "http-server": "^14.1.1",
    "markdownlint-cli2": "^0.21.0",
    "md-to-pdf": "^5.2.5",
    "prisma": "^5.22.0",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3"
  }
}
